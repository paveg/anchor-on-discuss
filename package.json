{
  "name": "anchor-on-discuss",
  "version": "1.0.0",
  "description": "Chrome extension to add anchor links to headings in GitHub Discussions",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "pnpm run generate-icons && tsc && vite build",
    "type-check": "tsc --noEmit",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "keywords": [
    "chrome-extension",
    "github",
    "anchor-links",
    "discussions"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
